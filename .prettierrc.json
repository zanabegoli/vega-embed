{
  "overrides": [
    {
      "files": "*",
      "options": {
        "printWidth": 120,
        "singleQuote": true,
        "trailingComma": "none"
      }
    },
    {
      "files": "*.js",
      "options": {
        "singleQuote": false
      }
    },
    {
      "files": "*.{css,scss,html}",
      "options": {
        "singleQuote": false
      }
    }
  ]
}
